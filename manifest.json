{
   "name": "Social Media Minimalist",
   "version": "0.0.3",
   "manifest_version": 3,
   "content_scripts": [
      {
         "run_at": "document_start",
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "index.js"
         ]
      }
   ],
   "permissions": [
      "scripting",
      "activeTab"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "override-fetch.js"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ]
}